{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "116 and West - Silverstripe 5.x Boilerplate",
    "require": {
        "php": "^8.1",
        "silverstripe/recipe-cms": "^5.0",
        "silverstripe/googlesitemaps": "^3.0",
        "kinglozzer/metatitle": "^3.0",
        "silverstripe/spamprotection": "^4.0.1",
        "undefinedoffset/silverstripe-nocaptcha": "^2.4.0",
        "jonom/focuspoint": "^5.0",
        "silverstripe/userforms": "^6.0.4",
        "symbiote/silverstripe-gridfieldextensions": "^4.0.2",
        "bummzack/sortablefile": "^2.2.0",
        "unclecheese/display-logic": "^3.0.0",
        "wedevelopnl/silverstripe-webp-images": "^2.0.0",
        "axllent/silverstripe-scaled-uploads": "^2.3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "classmap": [
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "psr-4": {
            "App\\": "app/src/"
        }
    },
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/database.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "phpcli": "php81",
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true,
            "silverstripe/recipe-plugin": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
